{
  "name": "@assignar/mysqldump",
  "version": "2.0.0-rc12",
  "description": "Create a DUMP from MySQL",
  "main": "dist/cjs.js",
  "module": "dist/es.js",
  "typings": "dist/mysqldump.d.ts",
  "scripts": {
    "test": "jest --verbose --config=./jest.json",
    "test-prod": "JEST_USE_NPM_BUILD=true jest --verbose --config=./jest.json",
    "view-coverage": "open ./coverage/index.html",
    "build": "rollup -c && dts-bundle-generator -o dist/mysqldump.d.ts build-decl/main.d.ts --umd-module-name=mysqldump && rimraf build-decl",
    "clean": "rimraf dist && rimraf build-decl",
    "lint": "eslint src --ext \".ts\"",
    "start": "ts-node -P ./tsconfig.run.json ./test/scripts/launch.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/assignar/mysqldump"
  },
  "keywords": [
    "backup",
    "mysql",
    "mysql-dump",
    "dump",
    "restore",
    "database",
    "backup",
    "database"
  ],
  "author": "Brad Zacher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/assignar/mysqldump/issues"
  },
  "homepage": "https://github.com/assignar/mysqldump",
  "dependencies": {
    "deepmerge": "^2.0.1",
    "mysql2": "^1.5.2",
    "sql-formatter": "^2.1.2",
    "sqlstring": "^2.3.1"
  },
  "devDependencies": {
    "@types/deepmerge": "^1.3.3",
    "@types/extend": "^3.0.0",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^22.1.4",
    "@types/mysql": "^2.15.2",
    "@types/sqlstring": "^2.2.0",
    "babel": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "dts-bundle-generator": "^0.6.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-typescript": "^0.9.0",
    "eslint-restricted-globals": "^0.1.1",
    "jest": "^22.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-typescript2": "^0.9.0",
    "ts-jest": "^22.4.0",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2",
    "typescript-eslint-parser": "^14.0.0"
  }
}
